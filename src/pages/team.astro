---
import BaseLayout from "../layouts/BaseLayout.astro";
import TimeLineElement from "../components/cv/TimeLine.astro";
---

<BaseLayout title="Team">
    <!-- Profile header -->

    <div class="mb-5">
        <div class="text-3xl w-full font-bold">Team</div>
    </div>

    <!-- Member 1 -->
    <div
        class="grid md:grid-cols-3 lg:grid-cols-2 gap-y-6 lg:gap-xl-12 text-center"
    >
        <div class="mb-12 lg:mb-0">
            <img
                class="rounded-lg shadow-lg mb-6 mx-auto"
                src="/rizal.jpg"
                alt="avatar"
                style="width: 150px; height: 150px; object-fit: cover; display: block; margin-left: auto; margin-right: auto;"
            />
            <h5 class="text-lg font-bold mb-4">Dr. Rizal Shidiq</h5>
            <p class="mb-1">Principal Investigator</p>
            <p class="mb-6">Leiden University Institute for Area Studies (LIAS)</p>
            <i class="bx bxs-home-alt-2"></i>
            <ul class="list-inside flex mx-auto justify-center">
                <!-- Social icons -->
                <a href="https://twitter.com/rizalshidiq?lang=en" class="px-2">
                    <svg
                        xmlns="http://www.w3.org/2000/svg"
                        viewBox="0 0 25 24"
                        class="w-6 h-6 text-blue-600"
                    >
                        <path
                            fill="lightblue"
